{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EonetClientOptions": {
    "BaseAddress": "https://eonet.gsfc.nasa.gov/api/v3"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2" // HTTP/1 enabled for gRPC Web, otherwise the whole Kestrel section of the config could be removed
    }
  }
}
